{
  "name": "mars-rover-photo-viewer",
  "version": "1.0.0",
  "description": "App to query and display image data gathered by NASA's Curiosity, Opportunity, and Spirit rovers on Mars",
  "main": "index.js",
  "repository": "git@github.com:timtroup/mars-rover-photo-viewer.git",
  "author": "Tim Troup",
  "license": "MIT",
  "scripts": {
    "test": "mocha --compilers js:babel-register --require ./test/helpers.js --require ./test/dom.js --recursive --require ignore-styles",
    "test-coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --require ./test/helpers.js --require ./test/dom.js --recursive --require ignore-styles",
    "build": "node ./node_modules/webpack/bin/webpack --config webpack.config.js",
    "clean": "yarn cache clean && del-cli node_modules dist",
    "clean:dist": "del-cli dist",
    "serve": "webpack-dev-server"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "cross-env": "^3.2.3",
    "css-loader": "^0.26.1",
    "del-cli": "^0.2.1",
    "enzyme": "^2.7.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "nock": "^9.0.9",
    "nyc": "^10.1.2",
    "proxyquire": "^1.7.11",
    "react-addons-test-utils": "^15.4.2",
    "redux-logger": "^2.8.1",
    "redux-mock-store": "^1.2.2",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "es6-promise": "^4.1.0",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.17.1",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-datepicker": "^0.40.0",
    "react-dom": "^15.4.2",
    "react-overlays": "^0.6.11",
    "react-photo-gallery": "^4.2.16",
    "react-redux": "^5.0.2",
    "react-spin": "^0.6.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.2"
  }
}
